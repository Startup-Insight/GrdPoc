@model IEnumerable<GrdPoc.Models.Entities.UserAccount>

@{
    ViewBag.Title = "Index";
}
<br />
@Html.Partial("../Shared/_AccountsTabsView")


<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MunicipalEntity.MunicipalEntityName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PersonaType.PersonaTypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserTitle)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MunicipalEntity.MunicipalEntityName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PersonaType.PersonaTypeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserTitle)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.UserAccountId }) |
            @Html.ActionLink("Details", "Details", new { id=item.UserAccountId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.UserAccountId })
        </td>
    </tr>
}

</table>
